<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.dnlab.domain.board.repository.BoardRepository">

    <select id="getBoardByNum" resultType="com.example.dnlab.domain.board.entity.Board">
        SELECT * FROM BOARD WHERE NUM = #{num}
    </select>
    <select id="getBoardList" resultType="com.example.dnlab.domain.board.entity.Board">
        SELECT * FROM board
    </select>
</mapper>